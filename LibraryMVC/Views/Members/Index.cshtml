@model IEnumerable<Member>

<button class="btn btn-primary" onclick="@("window.location.href='" + @Url.Action("AddMember", "Members") + "'");">Add Member</button>
<table class="table table-dark">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Member member in Model)
        {
            <tr>
                <td>@member.FirstName</td>
                <td>@member.LastName</td>
                <td>
                    <button class="btn btn-primary" onclick="@("window.location.href='" + @Url.Action("Edit", "Members", new { id = member.Id }) + "'");">Edit</button>
                    <button class="btn btn-danger" onclick="@("window.location.href='" + @Url.Action("Delete", "Members", new { id = member.Id }) + "'");">Delete</button>
                </td>
            </tr>
        }
    </tbody>
</table>